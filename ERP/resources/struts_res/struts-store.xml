<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<package name="store" extends="erp_base">
		<!-- storeAction-->
		<action name="store_*" class="storeAction" method="{1}">
			<result name="inList">WEB-INF/jsps/store/in/inList.jsp</result>
			<result name="storeList" type="json">
				<param name="root">storeList</param>
				
			</result>
		</action>
	</package>
</struts>
